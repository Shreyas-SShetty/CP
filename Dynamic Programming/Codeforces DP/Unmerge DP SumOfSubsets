int findInString(string &str, char ch) {
    int n = str.length();
    for(int i=0; i<n; ++i) {
        if(str[i] == ch) return i;
    }
    return -1;
}

void rf()
{
    #ifndef ONLINE_JUDGE
    freopen("input.txt", "r", stdin);
    freopen("output.txt", "w", stdout);
    #endif
}

void solve() {
    int n;
    cin >> n;
    vt<int> a(2*n);
    for(auto &it: a)
        cin >> it;
    vt<int> segments;
    int i = 0;
    while(i < 2*n) {
        int cur_count = 1;
        int j = i+1;
        while(j < 2*n and a[j] < a[i]) {
            ++cur_count;
            ++j;
        }
        segments.pb(cur_count);
        i = j;
    }
    int target = n;
    bitset<N> bt;
    for(auto x: segments) {
        bt = bt | (bt<<x);
        bt.set(x);
    }

    if(bt[target])
        cout<<"YES\n";
    else
        cout<<"NO\n";
}

//Thought  abt edge cases (n == 1??????????????)

int32_t main()
{
    fast;
    //cin.exceptions(cin.failbit);
   // rf();
    w(t) 
    {
        solve();
    }
}
