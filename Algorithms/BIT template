class Fentree {
private:
    vt<int> bit;
public:
    Fentree(){}
    Fentree(int n) {
        bit = vt<int> (n+100, 0);
    }
    void update(int pos, int val) {
        for(int i = pos; i < bit.size(); i += (i & (-i))) {
            bit[i] += val;
        }
    }
    int query(int pos) {
        int smallerOrEq = 0;
        for(int i = pos; i > 0; i -= (i & (-i))) {
            smallerOrEq += bit[i];
        }
        return smallerOrEq;
    }
};
